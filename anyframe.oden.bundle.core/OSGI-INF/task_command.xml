<?xml version="1.0" encoding="UTF-8"?>
<component name="TaskCommand" immediate="true" >
	<implementation class="anyframe.oden.bundle.core.command.TaskCommandImpl"/>
	<service>
		<provide interface="org.ungoverned.osgi.service.shell.Command"/>
	</service>
	<reference name="REPOSITORY_PROVIDER_SERVICE"
         interface="anyframe.oden.bundle.core.RepositoryProviderService"
         cardinality="1..1"
         policy="dynamic"
         bind="setRepositoryProvider"
   />
   <reference name="PERSISTENCE_SERVICE"
         interface="anyframe.oden.bundle.prefs.PrefsService"
         cardinality="1..1"
         policy="dynamic"
         bind="setPrefsService"
   />
   <reference name="ODEN_CONFIG_SERVICE"
         interface="anyframe.oden.bundle.core.config.OdenConfigService"
         cardinality="1..1"
         policy="dynamic"
         bind="setConfigService"
   />
   <reference name="POLICY_COMMAND"
         interface="org.ungoverned.osgi.service.shell.Command"
         cardinality="1..n"
         policy="dynamic"
         bind="setPolicyCommand"
   />
   <reference name="DEPLOY_LOG_SERVICE"
         interface="anyframe.oden.bundle.core.record.DeployLogService2"
         cardinality="1..1"
         policy="dynamic"
         bind="setDeployLogService"
   />
   <reference name="JOB_MANAGER"
         interface="anyframe.oden.bundle.core.job.JobManager"
         cardinality="1..1"
         policy="dynamic"
         bind="setJobManager"
   />
</component>
