<?xml version="1.0" encoding="UTF-8"?>
<component name="SnapshotCommand" immediate="true" >
	<implementation class="anyframe.oden.bundle.core.command.SnapshotCommandImpl"/>
	<service>
		<provide interface="org.ungoverned.osgi.service.shell.Command"/>
	</service>
	<reference name="DELEGATE_SERVICE"
         interface="anyframe.oden.bundle.core.DelegateService"
         cardinality="0..1"
         policy="dynamic"
         bind="setDelegateService"
         unbind="unsetDelegateService"
   />
   <reference name="PERSISTENCE_SERVICE"
         interface="anyframe.oden.bundle.prefs.PrefsService"
         cardinality="0..1"
         policy="dynamic"
         bind="setPrefsService"
         unbind="unsetPrefsService"
   />
   <reference name="ODEN_CONFIG_SERVICE"
         interface="anyframe.oden.bundle.core.config.OdenConfigService"
         cardinality="0..1"
         policy="dynamic"
         bind="setConfigService"
         unbind="unsetConfigService"
   />   
</component>
