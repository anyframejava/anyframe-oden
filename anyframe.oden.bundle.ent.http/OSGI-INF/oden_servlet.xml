<?xml version="1.0" encoding="UTF-8"?>
<component name="OdenServlet" immediate="true" >
	<implementation class="anyframe.oden.bundle.ent.http.OdenServlet"/>
	<reference name="HTTP_SERVICE"
         interface="org.osgi.service.http.HttpService"
         cardinality="1..1"
         policy="dynamic"
         bind="setHttpService"
   />
   <reference name="SECURITY_HANDLER"
         interface="anyframe.oden.bundle.ent.http.SecurityHandler"
         cardinality="0..1"
         policy="dynamic"
         bind="setSecurityHandler"
         unbind="unsetSecurityHandler"
   />
   <reference name="WEB_SERVICE"
         interface="anyframe.oden.bundle.ent.http.WebService"
         cardinality="0..n"
         policy="dynamic"
         bind="addWebService"
         unbind="removeWebService"
   />
</component>
