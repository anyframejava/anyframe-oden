<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
	<bean id="messageSource"
		class="org.anyframe.spring.message.EncodingResourceBundleMessageSource">
		<property name="basenames">
			<list>
				<value>message/message-service</value>
				<value>message/message-ui</value>
				<value>message/message-ui_ko</value>
				<value>message/message-ui_en</value>
				<value>anyframe/iam/core/messages/security</value>
			</list>
		</property>
		<property name="defaultEncoding">
			<value>UTF-8</value>
		</property>
	</bean>
</beans>
