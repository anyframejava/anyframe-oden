<?xml version="1.0" encoding="UTF-8"?>
<component name="SnapshotCommand" immediate="true" >
	<implementation class="anyframe.oden.bundle.ent.snapshot.SnapshotCommandImpl"/>
	<service>
		<provide interface="anyframe.common.bundle.gate.CustomCommand"/>
	</service>
   <reference name="PERSISTENCE_SERVICE"
         interface="anyframe.oden.bundle.core.prefs.PrefsService"
         cardinality="1..1"
         policy="dynamic"
         bind="setPrefsService"
   />
   <reference name="ODEN_CONFIG_SERVICE"
         interface="anyframe.oden.bundle.core.config.OdenConfigService"
         cardinality="1..1"
         policy="dynamic"
         bind="setConfigService"
   />   
   <reference name="TRANSMITTER_SERVICE"
         interface="anyframe.oden.bundle.core.txmitter.TransmitterService"
         cardinality="1..1"
         policy="dynamic"
         bind="setTransmitterService"
   />
   <reference name="DEPLOY_LOG_SERVICE"
         interface="anyframe.oden.bundle.core.record.DeployLogService"
         cardinality="1..1"
         policy="dynamic"
         bind="setDeployLogService"
   />
   <reference name="JOB_MANAGER"
         interface="anyframe.oden.bundle.core.job.JobManager"
         cardinality="1..1"
         policy="dynamic"
         bind="setJobManager"
   />
   <reference name="KEY_GENERATOR"
         interface="anyframe.oden.bundle.core.keygen.KeyGenerator"
         cardinality="1..1"
         policy="dynamic"
         bind="setKeyGenerator"
   />
</component>
