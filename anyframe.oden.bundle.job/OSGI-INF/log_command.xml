<?xml version="1.0" encoding="UTF-8"?>
<component name="LogCommand" immediate="true" >
	<implementation class="anyframe.oden.bundle.job.LogCommandImpl"/>
	<service>
		<provide interface="anyframe.common.bundle.gate.CustomCommand"/>
	</service>
	<reference name="Job_LOGGER_SERVICE"
         interface="anyframe.oden.bundle.job.log.JobLogService"
         cardinality="1..1"
         policy="dynamic"
         bind="setJobLogService"
	/>
	<reference name="PAGE_HANDLER"
         interface="anyframe.oden.bundle.job.page.PageHandler"
         cardinality="1..1"
         policy="dynamic"
         bind="setPageHandler"
	/>
</component>
